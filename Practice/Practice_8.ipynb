   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.random.randn(6,6)\n",
    "print(A)\n",
    "U,R = np.linalg.qr(A)\n",
    "Q,_R = np.linalg.qr(U)\n",
    "\n",
    "print(np.allclose(_R,np.eye(6))) # True라면 Identity Matrix 가 맞다."
   ]
